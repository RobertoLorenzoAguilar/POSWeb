@model EmpresaViewModel


<div class=" card text-primary">
    <div class=" card-header text-center ">
        <h5>Empresa</h5>
    </div>
    <div class=" card-body ">
        <dl class="row">
            <dt class="col-sm-2 text-center text-dark">
                Razon Social Empresa
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.RazonSocialEmpresa)
            </dd>

            <dt class="col-sm-2 text-center text-dark">
                Calle
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CalleDireccionEmpresa)
            </dd>

            <dt class="col-sm-2 text-center text-dark">
                Número
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NumeroDireccionEmpresa)
            </dd>

            <dt class="col-sm-2 text-center text-dark">
                Ciudad
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CiudadEmpresa)
            </dd>

            <dt class="col-sm-2 text-center text-dark">
                RFC
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.RfcEmpresa)
            </dd>
        </dl>
    </div>
    <div class=" card-footer text-center">
        <a asp-action="Edit" asp-route-id="@Model.IdEmpresa" class="btn btn-outline-warning btn-sm">Editar</a>
        <a asp-action="Index" class=" btn btn-outline-success btn-sm">volver a la lista</a>
    </div>
</div>
